{
  "name": "scrapy",
  "version": "0.1.0",
  "description": "web scraping package for node",
  "main": "./src/scraper.js",
  "scripts": {
    "test": "nyc mocha",
    "build": "jshint src/*.js test/*.js && nyc mocha"
  },
  "author": "Dmitry Lukashevich",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "nyc": "^10.2.0",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "ramda": "^0.23.0",
    "winston": "^2.3.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "unused": true,
    "node": true,
    "mocha": true,
    "eqeqeq": true,
    "maxdepth": 2,
    "maxparams": 3,
    "noarg": true,
    "nocomma": true,
    "nonew": true,
    "varstmt": true,
    "undef": true,
    "quotmark": "double"
  }
}
